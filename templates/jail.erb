[<%= @name %>]
enabled = <%= @enabled %>
port = <%= @port %>
filter = <%= @filter %>
logpath = <%= @logpath %>
<% if @protocol != false -%>protocol = <%= @protocol %>
<% end -%>
<% if @maxretry != false -%>maxretry = <%= @maxretry %>
<% end -%>
<% if @findtime != false -%>findtime = <%= @findtime %>
<% end -%>
<% if @action != false -%>action = <%= @action %>
<% end -%>
<% if @banaction != false -%>banaction = <%= @banaction %>
<% end -%>
<% if @bantime != false -%>bantime = <%= @bantime %>
<% end -%>
<% if @ignoreip != false -%>ignoreip = <%= @ignoreip %>
<% if @backend == 'systemd' -%>
backend = <%= @backend %>
journalmatch = <%= @journalmatch %>
<% else -%>
backend = <%= backend %>
<% end -%>

